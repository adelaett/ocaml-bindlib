let And = λA,B:*.∀X:*. ((A → B → X) → X) : ∀A,B:*. *;

let pair = λA,B:*.λa:A.λb:B.λX:*.λf:A → B → X. f a b :
   ∀A,B:*. A → B → And A B;

let fst = λA,B:*.λp: And A B. p A (λx:A.λy:B. x) : ∀A,B:*. And A B → A;
let snd = λA,B:*.λp: And A B. p B (λx:A.λy:B. y) : ∀A,B:*. And A B → B;

let pn = Nat → * : #;

let True = Idt : *;
